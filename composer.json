{
    "name": "fp/jsformvalidator-bundle",
    "type": "symfony-bundle",
    "description": "Javascript validation for sf2 forms.",
    "keywords": ["Symfony", "Form", "Javascript", "Validation"],
    "homepage": "https://github.com/formapro/JsFormValidatorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ton Sharp",
            "email": "66ton99@gmail.com"
        },
        {
            "name": "Yury Maltsev",
            "email": "dev.yury.maltsev@gmail.com"
        }

    ],

    "require": {
        "php": ">=5.3.2",
        "symfony/form": ">=2.3",
        "symfony/validator": "~2.3.0,>=2.3.18||~2.4.0,>=2.4.8||~2.5.0,>=2.5.2"
    },

    "require-dev": {
        "doctrine/orm": ">=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/symfony": "~2.3.0,>=2.3.18||~2.4.0,>=2.4.8||~2.5.0,>=2.5.2",
        "symfony/assetic-bundle": ">=2.3",
        "phpunit/phpunit": "3.7.*",
        "behat/mink-bundle": "dev-master",
        "behat/mink-selenium2-driver": "1.1.0",
        "satooshi/php-coveralls": "v1.0.0",
        "se/selenium-server-standalone": "2.45.0"
    },

    "suggest": {
        "doctrine/orm": "For the UniqueEntity validation",
        "doctrine/doctrine-bundle": "For the UniqueEntity validation",
        "symfony/symfony": "For using as a Symfony Bundle and for using a default ajax validation",
        "symfony/assetic-bundle": "For easy enabling"
    },

    "autoload": {
        "psr-0": { "Fp\\JsFormValidatorBundle": "" }
    },

    "target-dir": "Fp/JsFormValidatorBundle",

    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
